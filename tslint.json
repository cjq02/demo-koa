{
    "extends": "tslint-config-airbnb",
    "rules": {
        "import-name": false
    },
    "ter-indent": [
        true,
        4,
        {
            "FunctionDeclaration": {
                "parameters": 1,
                "body": 1
            },
            "FunctionExpression": {
                "parameters": 1,
                "body": 1
            },
            "MemberExpression": 1
        }
    ],
}